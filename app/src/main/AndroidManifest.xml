<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.autism.mindpower.feeling">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".ContactsActivity"
            android:label="@string/contactActivity" />
        <activity
            android:name=".PasswordCreateActivity"
            android:label="@string/set_password"
            android:noHistory="true" />
        <activity
            android:name=".SettingActivity"
            android:label="@string/action_settings" />
        <activity
            android:name=".PasswordCheckActivity"
            android:label="Wait..."
            android:noHistory="true" />
        <activity android:name=".ContactCheckListActivity" />
        <activity android:name=".FragmentPager" />
        <activity android:name=".PagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>